        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@stablelib/base64": "^1.0.1",
        "@types/msgpack-lite": "^0.1.8",
        "dotenv": "^8.2.0",
        "electron-log": "^4.4.1",
        "express": "^4.18.1",
        "helmet": "^5.1.1",
        "morgan": "^1.10.0",
        "msgpack-lite": "^0.1.26",
        "tweetnacl": "^1.0.3"
